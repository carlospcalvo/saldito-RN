{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["assets/*"],
			"@constants/*": ["constants/*"],
			"@contexts/*": ["contexts/*"],
			"@components/*": ["components/*"],
			"@hooks/*": ["hooks/*"],
			"@lib/*": ["lib/*"],
			"@model/*": ["model/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"model/schema.ts",
		"model/models.js",
		"lib/helpers/debounce.js"
	],
	"exclude": ["node_modules"]
}
